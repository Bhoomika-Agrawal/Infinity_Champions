class Solution {
    public int rob(int[] nums) {
        int[] dp=new int[nums.length];
        Arrays.fill(dp,-1);
        return find(nums,dp,0);
    }
    public static int find(int[] nums,int[] dp,int i){
        if(i>=nums.length) return 0;
        if(dp[i]!=-1) return dp[i];
        int dont_rob=find(nums,dp,i+1);
        int rob=nums[i]+find(nums,dp,i+2);
        return dp[i]=Math.max(dont_rob,rob);
    }
}
